  <nav class="navbar navbar-expand-lg navbar-light bg-light nav-changes">
    <div class="container-fluid">
      <a class="navbar-brand" href="#">Navbar</a>
      <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarSupportedContent">
    <ul class="navbar-nav me-auto mb-2 mb-lg-0">
      {% for link in linklists[section.settings.menu].links %}
        {% if link.links != blank %}
          <li class="nav-item dropdown">
            <a href="{{ link.url }}" class="nav-link" id="navbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
              {{ link.title }}
            </a>
            <ul class="dropdown-menu nav-ul-changes" aria-labelledby="navbarDropdown">
              {% for childlink in link.links %}
                <li class="nav-item nav-li-changes">
                  <a href="{{ childlink.url }}" class="nav-link">
                    {{ childlink.title }}
                  </a>
                </li>
              {% endfor %}
            </ul>
          </li>
        {% else %}
          <li class="nav-item">
            <a href="{{ link.url }}" class="nav-link">
              {{ link.title }}
            </a>
          </li>
        {% endif %}
      {% endfor %}
    </ul>
    <form class="d-flex">
      <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
      <button class="btn btn-outline-success" type="submit">Search</button>
